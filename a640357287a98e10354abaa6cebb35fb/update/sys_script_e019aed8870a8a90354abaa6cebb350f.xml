        <script><![CDATA[(function executeRule(current, previous /*null when async*/ ) {

    var serviceVehTracker = current.vehicle_tracker; //get sys id of vehicle tracker associated in ticket

    if (serviceVehTracker) { //if vehicle tracker record found, proceed
        var grTrackerStatus = new GlideRecord('x_1327411_loaner_v_vehicle_tracker'); //set querying table
        if (grTrackerStatus.get(serviceVehTracker)) { //getting this record
            grTrackerStatus.setValue('vehicle_status', current.vehicle_status); //Set the value of the vehicle status field on the GlideRecord to the current vehicle status on the servicing table 
            grTrackerStatus.setValue('tracker_status', 'closed'); //Set the value of the tracker status field to closed on the GlideRecord

            grTrackerStatus.update();

        }


    }

})(current, previous);]]></script>
